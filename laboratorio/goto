#include <stdio.h>
#include <time.h>
#include <stdlib.h>
#include <windows.h>

void gotoxy(int x,int y){
    COORD coordinate={x,y};
    HANDLE console=GetStdHandle(STD_OUTPUT_HANDLE);
    SetConsoleCursorPosition(console,coordinate);
}


int main() {
	/*gotoxy(49,10);
    printf("Ciao 4B");
    gotoxy(40,11);
    printf("mi chiamo Pierluigi Casciaro");
    gotoxy(40,11);
    printf("                                      ");*/

int i,x;

for(i=0;i<5;i++){
	gotoxy(x,10);
	printf("*");
	gotoxy(x,11);
	printf("*");
	gotoxy(x,12);
	printf("*");
	gotoxy(x,13);
	printf("*");
	x++; 
}	
return 0;
}	
