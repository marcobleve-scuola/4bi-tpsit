#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Inserisci numero1: ";
    cin >> a;
    cout << "Inserisci numero2: ";
    cin >> b;

    int somma = a + b;
    cout << "La somma è: " << somma << endl;
    int sott= a-b;
    cout << "La sottrazione è: " << sott << endl;

    return 0;
}
